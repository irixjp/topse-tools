import hudson.model.*

node {
  // The last stage cleans up the local images and workspace
  stage('Housekeeping') {

    deleteDir()
    sh "echo ${env.BRANCH_NAME}"

  }

  // The first stage checks out the code from Github
  stage('Get Code') {
    sh "nova --help"
    checkout scm
    sh "find ."

  }

  // Get some code from a GitHub repository


}

